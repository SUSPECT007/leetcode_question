class Solution {
public:
    vector<int> searchRange(vector<int>& nums, int target) {
        int f=-1,l=-1,i=0;
        for(i=0;i<nums.size();++i){
            if(target>nums[i]) continue;
            if(target<nums[i]) break;
            if(f==-1&&target==nums[i]) f=i;
        }
        if(f!=-1) l=i-1;
        vector<int> ans;
        ans.push_back(f);ans.push_back(l);
        return ans;
    }
};
