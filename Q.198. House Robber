class Solution {
public:
    int rob(vector<int>& nums) {
        int n=nums.size(),rob=0,nrob=0;
        int newrob,newnrob;
        if (n==1) return nums[0];
        for(int i=0;i<n;++i){
            newrob=nrob+nums[i];
            newnrob=max(rob,nrob);
            rob=newrob;
            nrob=newnrob;

        }
        return max(rob,nrob);
    }
};
