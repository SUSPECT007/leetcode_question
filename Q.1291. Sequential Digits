class Solution {
public:
    vector<int> sequentialDigits(int low, int high) {
        vector<int> ans;
        int nextdigit,num;
        for(int i=1;i<=9;++i){
            num=i;
            nextdigit=i+1;
            while(num<=high && nextdigit<=9){
                num=num*10+nextdigit;
                ++nextdigit;
                if(num>=low&&num<=high) ans.push_back(num);
            }

        }
        sort(ans.begin(),ans.end());
        return ans;
    }
};
