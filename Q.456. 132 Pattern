class Solution {
public:
    bool find132pattern(vector<int>& nums) {
        int n=nums.size();if(n<3) return false;
        stack<int> middle;
        int third=INT_MIN;
        for(int i=n-1;i>=0;--i){
            if(nums[i]<third) return true;
            while(!middle.empty()&&nums[i]>middle.top()){
                third=middle.top();
                middle.pop();

            }
            middle.push(nums[i]);


        }
        return false;
    }
};
